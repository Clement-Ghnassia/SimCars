
public abstract class ConfigMoteur {
	public static int MOTEUR_ESSENCE_VITESSE_MAX = 100;
	public static int MOTEUR_ELECTRIQUE_VITESSE_MAX = 60;
	
	public static double MOTEUR_ESSENCE_ACCELERATION = 80;
	public static int MOTEUR_ELECTRIQUE_ACCELERATION = 100; 
	
	public static double MOTEUR_ESSENCE_CONSOMMATION = 0.25;
	public static double MOTEUR_ELECTRIQUE_CONSOMMATION = 0.15;
}
