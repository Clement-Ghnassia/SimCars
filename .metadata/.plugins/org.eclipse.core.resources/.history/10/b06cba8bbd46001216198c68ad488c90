
public abstract class Voiture {
	protected int id; 
	
	protected int cVitesse;
	
	public static int VITESSE_MAX;
	public static int CAPACITE_FREINAGE;
	public static int HABILITE;
		
	public abstract boolean hasToFill();
	
	public int getType() {
		return this.id;
	}
	
	public void setId(int pId) {
		this.id = pId;
	}
	
	public int getCVitesse() {
		return this.cVitesse;
	}
	
	public void setCVitesse(int pCVitesse) {
		this.cVitesse = pCVitesse;
	}
	
	public static void accelere();
	
}
