
public abstract class Voiture {
	protected int id;
	protected int speed;
		
	public abstract boolean hasToFill();
	
	public int getId() {
		return this.id;
	}
	
	public void setId(int pId) {
		this.id = pId;
	}
	
	public int getSpeed() {
		return this.speed;
	}
	
	public void setSpeed(int pSpeed) {
		this.speed = pSpeed;
	}
	
}
