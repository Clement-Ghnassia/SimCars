
public class Moteur {
	
	protected TypeMoteur type;
	protected int vitesseMax;
	protected int acceleration;
	
	public Moteur(TypeMoteur pType) {
		this.type = pType;
		if(pType == TypeMoteur.TYPE_ELECTRIQUE) {
			this.vitesseMax = ConfigMoteur.MOTEUR_ESSENCE_VITESSE_MAX;
			this.acceleration = ConfigMoteur.MOTEUR_ESSENCE_ACCELERATION;
			
		}
	}
	
	public static int accelere(int vitesse) {
		
		int res;
		
		
		
		if(vitesse == MoteurEssence.VITESSE_MAX) {
			res = vitesse;
		}
	}
}
